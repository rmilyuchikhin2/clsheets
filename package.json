{
  "name": "clsheets",
  "version": "0.1.0",
  "description": "Store Google Sheets locally",
  "main": "./src/index.js",
  "scripts": {
    "build": "npm run build-local && npm i -g --loglevel=error",
    "build-fresh": "npm cache clean --force && npm i && npm run build",
    "build-local": "tsc --project tsconfig.json",
    "watch": "tsc --project tsconfig.json --watch",
    "publish": "npm publish --access public",
    "lint": "tslint --project tslint.json && echo 'No lint errors. All good!'",
    "prettier": "prettier --parser typescript --single-quote --bracket-spacing --print-width 110 --trailing-comma all src/**/*.ts --write"
  },
  "engines": {
    "node": ">=8.2.1"
  },
  "bin": {
    "clsheets": "./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rmilyuchikhin2/clsheets"
  },
  "author": "Roman Milyuchikhin",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/cli-spinner": "^0.2.0",
    "@types/fs-extra": "^9.0.2",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^14.11.10",
    "@types/pluralize": "0.0.29",
    "prettier": "^2.1.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "cli-spinner": "^0.2.10",
    "commander": "^3.0.1",
    "dotf": "^1.5.2",
    "find-up": "^5.0.0",
    "fs-extra": "^9.0.1",
    "google-auth-library": "^6.1.1",
    "googleapis": "^61.0.0",
    "is-online": "^8.5.0",
    "mkdirp": "^0.5.1",
    "open": "^7.3.0",
    "pluralize": "^8.0.0"
  }
}
